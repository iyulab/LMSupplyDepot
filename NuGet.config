<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>

  <!-- Package source mapping to resolve NU1507 warning -->
  <packageSourceMapping>
    <!-- Default packages from nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>

  <disabledPackageSources>
    <!-- Disable package sources here if needed -->
  </disabledPackageSources>
</configuration>
